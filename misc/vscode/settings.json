{
    "workbench.colorTheme": "DarkRoomNormal",
    "editor.fontSize": 11,
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.lineHeight": 14,
    "files.exclude": {
        "**/*.pyc": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.expo": true,
        "**/node_modules": true,
        "**/dist": true,
        "**/.idea": true,
        "**/.venv": true,
        "**/__pycache__": true,
        "**/.vscode": true,
        "**/build": true,
        "misc/cube/": true,
        "staticfiles/": true,
        "data/postgres/": true
    },
    "python.pythonPath": "${workspaceRoot}/.venv/bin/python",
    "editor.minimap.enabled": false,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.fontSize": 12,
    "python.linting.flake8Args": [
        "--disable=E501"
    ],
    "files.insertFinalNewline": true,
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": 0,
    "files.associations": {
        "*.js": "javascriptreact"
    },
}
